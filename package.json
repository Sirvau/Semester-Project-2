{
  "name": "semester_project_2_2024",
  "version": "0.0.16",
  "description": "Auction site application",
  "main": "index.js",
  "scripts": {
    "build": "sass src/scss:dist/css",
    "watch": "sass --watch src/scss:dist/css",
    "start": "live-server",
    "dev": "npm-run-all --parallel watch start",
    "test": "",
    "lint": "eslint src/**/*.js",
    "lint-fix": "eslint src/**/*.js --cache --fix",
    "format": "prettier -w src/**/*.js",
    "prepare": "husky install"
  },
  "keywords": [
    "Bootstrap",
    "SCSS",
    "Website"
  ],
  "author": "Siril O. Vaular",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.1.1",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "live-server": "^1.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "sass": "^1.75.0"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --cache --fix"
    ],
    "*.html": [
      "prettier --write"
    ],
    "*.scss": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "bootstrap": "5.3"
  }
}
